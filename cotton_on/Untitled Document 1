<script>
$(function(){

  // datepicker for birthdate popup
  var date_field = $('#subscribe label:contains("Date")').parent().children('input');

  var test_el = document.createElement("input");
  test_el.setAttribute('type', 'date');

  // Settings the type will fail in IE/unsupported browsers
  if(test_el.type === 'text' && date_field.length > 0) {
    date_field.datepicker({
      
      dateFormat:'yy-mm-dd'
      
    });
  }

  //init minicart scrollbar
  $('#cartview .items').jScrollPane();

  //placeholder polyfil for search
  $('#search_input').placeholder();

  //region change messaging
  $('.regionselect a[class!="active"]').on('click', function(){

    var currentRegion = 'Australia';
    var url = $(this).attr('href');

    // If the same region, just change site without
    // any warnings
    if ($(this).data("region") === currentRegion) {
      window.location = url;
      return false;
    }

    if ($('#cart-quantity').html() != "0 items") {
      $.colorbox({
        inline:true,
        href: '#msg_region_change',
        opacity: 0.6,
        transition:'none',
        open: true,
        onComplete: function() {
          $('.region_continue').click(function(){
            window.location = url;
          });
          $('.region_cancel').click(function(){
            $.colorbox.close();
          });
        }
      });
      return false;
    }
  });

});
</script>


<div id="site_msg">
  <div id="msg_region_change">

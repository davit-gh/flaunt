  function vmeCallback(VmeEventType, eventData){
    var MerchantTrans = eventData.merchTrans;
    if (VmeEventType == "purchase.success"){
      
      postToURL("https://shop.cottonon.com/shop/return-from-checkout-with-vme/", eventData);
      
    }
    else if(VmeEventType == "purchase.init"){
      //fires when payment widget is launched, update conversion tracking if applicable
    }
    else if(VmeEventType == "purchase.cancel"){
      // TODO fires when customer closes payment widget before completing purchase, update conversion tracking if applicable
    }
    else if(VmeEventType == "purchase.pending"){
      // TODO fires when shipping address or payment info validation fails, inform customer accordingly
    }
    else{
      // error handling
    }
  }
function postToURL(path, params){
  var form = document.createElement("form");
  form.setAttribute("method", "post");
  form.setAttribute("action", path);

    for(var key in params){
      if(params.hasOwnProperty(key)){
        var hiddenField = document.createElement("input");
        hiddenField.setAttribute("type", "hidden");
        hiddenField.setAttribute("name", key);
        hiddenField.setAttribute("value", params[key]);
        form.appendChild(hiddenField);
      }
    }
    document.body.appendChild(form);
    form.submit();
  }
</script>

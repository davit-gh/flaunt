<script>
  $("#cart form").validate();

  var URL_INCREASE = "/cart/ajax/increase/", URL_DECREASE = "/cart/ajax/decrease/", URL_QUANTITY = "/cart/ajax/quantity/";
  var URL_SHIPPING = "/cart/ajax/shipping/";
  $(function() {
    initCartPage();

    $('select').selectBox();
    // Disable alpha input
    $('.qtyfield').find('input').keydown(function(event) {
      // Allow: backspace, delete, tab, escape, and enter
      if ( event.keyCode == 46 || event.keyCode == 8 || event.keyCode == 9 || event.keyCode == 27 || event.keyCode == 13 ||
           // Allow: Ctrl+A
           (event.keyCode == 65 && event.ctrlKey === true) ||
           // Allow: home, end, left, right
           (event.keyCode >= 35 && event.keyCode <= 39)) {
        // let it happen, don't do anything
        return;
      }
      else {
        // Ensure that it is a number and stop the keypress
        if (event.shiftKey || (event.keyCode < 48 || event.keyCode > 57) && (event.keyCode < 96 || event.keyCode > 105 )) {
          event.preventDefault();
        }
      }
    });

    $('.verify').click(function(evt){
      verifyDiscount(
          "/shop/cart/",
          "aJE3pC09ZkVnCDzSfqh2yezWw6lRLEgJ"
      )
      _gaq.push(['_trackEvent', 'Discount', 'Discount Code', 'Verify button click']);
      evt.preventDefault();
    });

    $('#id_discount_code').focus(function(){
      _gaq.push(['_trackEvent', 'Discount', 'Discount Code', 'Input focus']);
    });
  });
</script>

  
